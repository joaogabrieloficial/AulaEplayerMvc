@model Noticias
@{
    ViewData["Title"] = "Noticias";
}

<form action="@Url.Action("Cadastrar")" method="post">

<label for="IdNoticia">IdNoticia</label>></label>
<input type="text" name="IdNoticia" id="IdNoticia">

<label for="Titulo">Titulo</label>></label>
<input type="text" name="Titulo" id="Titulo">

<label for="Texto">Texto</label>></label>
<input type="text" name="Texto" id="Texto">

<label for="Imagem">Imagem</label>></label>
<input type="text" name="Imagem" id="Imagem">

<button type="submit">Cadastrar</button>



</form>

<table class="table table-responsive table-striped">

<thead>
    <th>Id</th>
    <th>Titulo</th>
    <th>Texto</th>
    <th>Imagem</th>
</thead>
<tbody>
    @foreach (var item in ViewBag.Noticias)
    {
        <tr>
            <td>@item.IdNoticia</td>
            <td>@item.Titulo</td>
            <td>@item.Texto</td>
            <td>@item.Imagem</td>
        </tr>
    }
</tbody>
</table>